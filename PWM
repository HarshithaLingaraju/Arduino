void setup() {
  pinMode(LED_BUILTIN, OUTPUT);
  Serial.begin(9600);
  Serial.println("PWM with LED");
}

void loop() {
 for (int i = 0; i <= 255; i++) {
    analogWrite(LED_BUILTIN, i);
    delay(100);
  }

  // Fade out
  for (int i = 255; i >= 0; i--) {
    analogWrite(LED_BUILTIN, i);
    delay(10);
  }
}
